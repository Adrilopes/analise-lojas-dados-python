      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# média das avaliações\n",
        "media_avaliacoes = lojas.groupby(\"Loja\")[\"Avaliação da compra\"].mean().round(2)\n",
        "\n",
        "# ordenar do maior para o menor\n",
        "media_ordenada = media_avaliacoes.sort_values(ascending=False)\n",
        "\n",
        "# a primeira da lista é a melhor loja\n",
        "melhor_loja = media_ordenada.index[0]\n",
        "\n",
        "# cores dos pontos\n",
        "cores = [\"red\" if loja == melhor_loja else \"gray\" for loja in media_ordenada.index]\n",
        "\n",
        "plt.figure(figsize=(8,5))\n",
        "\n",
        "# linha principal\n",
        "plt.plot(media_ordenada.index, media_ordenada.values, marker='o', color='black', linewidth=2)\n",
        "\n",
        "# pontos com cores específicas\n",
        "plt.scatter(media_ordenada.index, media_ordenada.values, color=cores, s=120)\n",
        "\n",
        "# título e labels\n",
        "plt.title(\"Média das Avaliações por Loja (Melhor → Pior)\")\n",
        "plt.xlabel(\"Loja\")\n",
        "plt.ylabel(\"Avaliação Média\")\n",
        "\n",
        "# escreve valores acima dos pontos\n",
        "for i, v in enumerate(media_ordenada.values):\n",
        "    plt.text(i, v + 0.02, str(v), ha='center', fontsize=10)\n",
        "\n",
        "plt.ylim(0, 5)\n",
        "plt.grid(alpha=0.3)\n",
        "plt.show()"
