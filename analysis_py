          "metadata": {},
          "execution_count": 16
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "\n",
        "def categorias_por_loja(dataset, nome_loja, cor):\n",
        "    # Contagem de categorias\n",
        "    data = dataset['Categoria do Produto'].value_counts().sort_values(ascending=True)\n",
        "\n",
        "    # Criar figura\n",
        "    plt.figure(figsize=(8, 5))\n",
        "    bars = plt.barh(data.index, data.values, color=cor)\n",
        "\n",
        "    # Título automático\n",
        "    plt.title(f\"Categorias Mais Vendidas – {nome_loja}\", fontsize=14)\n",
        "\n",
        "    # Labels\n",
        "    plt.xlabel(\"Quantidade de Vendas\")\n",
        "    plt.ylabel(\"Categoria\")\n",
        "\n",
        "    # Adicionar valores numéricos nas barras\n",
        "    for bar in bars:\n",
        "        width = bar.get_width()\n",
        "        plt.text(width + 1,                     # posição X\n",
        "                 bar.get_y() + bar.get_height()/2,  # posição Y\n",
        "                 str(int(width)),               # valor\n",
        "                 va='center')\n",
        "\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
        "\n",
        "    return data"
      ],
      "metadata": {
        "id": "yZR6cOjXYSk1"
      },
      "execution_count": null,
      "outputs": []
    },
