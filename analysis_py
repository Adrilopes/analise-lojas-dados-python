        "# 4. Produtos Mais e Menos Vendidos"
      ],
      "metadata": {
        "id": "DmNp1KPBZCm-"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def mais_vendas(data, loja_nome):\n",
        "    cont1 = data['Produto'].value_counts().head(1)   # produto mais vendido\n",
        "    cont2 = data['Produto'].value_counts().tail(1)   # produto menos vendido\n",
        "    a = pd.concat([cont1, cont2])\n",
        "    a.plot(kind='bar', figsize=(7, 5), color='red')\n",
        "    plt.xlabel(\"\")\n",
        "    plt.xticks(rotation=0)\n",
        "    plt.title(f\"Produto mais e menos vendido da {loja_nome}\")\n",
        "    plt.show()"
      ],
      "metadata": {
        "id": "qoyjqijQZHfF",
        "collapsed": true
      },
      "execution_count": null,
      "outputs": []
    },
